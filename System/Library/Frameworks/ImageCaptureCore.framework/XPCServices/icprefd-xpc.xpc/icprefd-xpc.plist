<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.apple.icprefd-xpc</string>
	<key>CFBundleName</key>
	<string>Handles ImageCaptureCore Authentication TCC/FileProvider</string>
	<key>com.apple.private.tcc.allow</key>
	<array>
		<string>kTCCServiceCamera</string>
	</array>
	<key>com.apple.private.tcc.manager.check-by-audit-token</key>
	<array>
		<string>kTCCServiceCamera</string>
	</array>
	<key>com.apple.private.tcc.override-prompt-policy</key>
	<true/>
	<key>com.apple.private.xpc.domain-extension</key>
	<true/>
