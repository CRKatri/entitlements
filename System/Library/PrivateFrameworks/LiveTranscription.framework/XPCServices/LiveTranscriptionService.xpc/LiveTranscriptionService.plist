		<string>kTCCServiceMicrophone</string>
		<string>kTCCServiceSpeechRecognition</string>
	</array>
	<key>com.apple.security.exception.shared-preference.read-write</key>
	<array>
		<string>com.apple.Accessibility</string>
	</array>
