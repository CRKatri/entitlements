<plist version="1.0">
<dict>
	<key>com.apple.assistant.dictation.prerecorded</key>
	<true/>
	<key>com.apple.coreaudio.app-tap</key>
	<true/>
	<key>com.apple.coreaudio.private.SystemWideTap</key>
	<true/>
